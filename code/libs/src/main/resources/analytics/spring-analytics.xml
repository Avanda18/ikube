    <bean
        class="org.springframework.beans.factory.config.MethodInvokingFactoryBean"
        property:staticMethod="ikube.analytics.AnalyzerManager.buildAnalyzers">
        <property name="arguments">
            <list>
                <ref local="context-em"/>
                <ref local="context-smo"/>
                <ref local="context-smo-en"/>
                <ref local="context-smo-es"/>
                <ref local="context-smo-jp"/>
                <ref local="context-smo-cn"/>
                <ref local="context-smo-nl"/>
                <ref local="context-smo-fr"/>
                <ref local="context-smo-de"/>
                <ref local="context-smo-sv"/>
                <ref local="context-smo-th"/>
                <ref local="context-smo-ar"/>
                <ref local="context-smo-tr"/>
                <ref local="context-smo-ru"/>
            </list>
        </property>
    </bean>

</beans>