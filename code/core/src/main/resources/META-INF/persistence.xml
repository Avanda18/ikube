    	http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd" version="1.0">

	
	<persistence-unit name="IkubePersistenceUnitObjectDb" transaction-type="RESOURCE_LOCAL">
		<provider>com.objectdb.jpa.Provider</provider>
		<class>ikube.model.Persistable</class>
		
		<class>ikube.model.faq.Faq</class>
		<class>ikube.model.faq.Attachment</class>
		
		<class>ikube.model.medical.Hospital</class>
		<class>ikube.model.medical.Administration</class>
		<class>ikube.model.medical.Person</class>
		<class>ikube.model.medical.Doctor</class>
		<class>ikube.model.medical.Patient</class>
		<class>ikube.model.medical.Inpatient</class>
		<class>ikube.model.medical.Condition</class>
		<class>ikube.model.medical.Medication</class>
		<class>ikube.model.medical.Treatment</class>
		<class>ikube.model.medical.Record</class>
		<class>ikube.model.medical.Address</class>
		
		<class>ikube.model.Url</class>
		<class>ikube.model.File</class>
		<class>ikube.model.Server</class>
		<class>ikube.model.IndexContext</class>
		<class>ikube.model.IndexableTable</class>
		<class>ikube.model.IndexableInternet</class>
		<class>ikube.model.IndexableFileSystem</class>
		<class>ikube.model.IndexableEmail</class>
		<class>ikube.model.IndexableColumn</class>
		<class>ikube.model.Indexable</class>
		<class>ikube.model.Index</class>
		<class>ikube.model.Execution</class>
		<class>ikube.model.Action</class>
		
		<class>ikube.model.geospatial.GeoName</class>
		<class>ikube.model.geospatial.AlternateName</class>
		<exclude-unlisted-classes>false</exclude-unlisted-classes>
		<properties>
			<property name="javax.persistence.jdbc.url" value="./ikube.odb" />
			<property name="javax.persistence.jdbc.user" value="sa" />
			<property name="javax.persistence.jdbc.password" value="" />
		</properties>
	</persistence-unit>
	
	<persistence-unit name="IkubePersistenceUnitH2" transaction-type="RESOURCE_LOCAL">
		<provider>org.apache.openjpa.persistence.PersistenceProviderImpl</provider>
		<non-jta-data-source>nonXaDataSourceH2</non-jta-data-source>
		<class>ikube.model.Persistable</class>
		
