<hazelcast>
	<group>
		<name>ikube</name>
		<password>ikube</password>
	</group>
	<network>
		<port auto-increment="true">8010</port>
		<join>
			<multicast enabled="true">
				<multicast-port>8010</multicast-port>
				<multicast-group>224.1.2.3</multicast-group>
			</multicast>
			<tcp-ip enabled="false">
				<interface>192.168.1.4</interface>
				<interface>192.168.1.8</interface>
			</tcp-ip>
		</join>
		<interfaces enabled="true">
			<interface>192.168.1.*</interface>
		</interfaces>
		<symmetric-encryption enabled="false">
			<algorithm>PBEWithMD5AndDES</algorithm>
			<salt>thesalt</salt>
			<password>thepass</password>
			<iteration-count>19</iteration-count>
		</symmetric-encryption>
		<asymmetric-encryption enabled="false">
			<algorithm>RSA/NONE/PKCS1PADDING</algorithm>
			<keyPassword>thekeypass</keyPassword>
			<keyAlias>local</keyAlias>
			<storeType>JKS</storeType>
			<storePassword>thestorepass</storePassword>
			<storePath>keystore</storePath>
		</asymmetric-encryption>
	</network>
	<executor-service>
		<core-pool-size>1000000</core-pool-size>
		<max-pool-size>1000000</max-pool-size>
		<keep-alive-seconds>60000</keep-alive-seconds>
	</executor-service>
	<map name="ikube">
		<backup-count>5</backup-count>
		<eviction-policy>LFU</eviction-policy>
		<max-size>10000</max-size>
		<eviction-percentage>25</eviction-percentage>
		<merge-policy>hz.ADD_NEW_ENTRY</merge-policy>
		<map-store enabled="false">
			<class-name>ikube.cluster.hzc.ClusterManagerCache</class-name>
			<write-delay-seconds>0</write-delay-seconds>
		</map-store>
	</map>
	<map name="search">
		<backup-count>5</backup-count>
		<eviction-policy>LFU</eviction-policy>
		<max-size>10000</max-size>
		<time-to-live-seconds>600</time-to-live-seconds>
		<eviction-percentage>25</eviction-percentage>
		<merge-policy>hz.ADD_NEW_ENTRY</merge-policy>
		<map-store enabled="false">
			<class-name>ikube.cluster.hzc.ClusterManagerCache</class-name>
			<write-delay-seconds>0</write-delay-seconds>
		</map-store>
	</map>
	<merge-policies>
	</merge-policies>

</hazelcast>