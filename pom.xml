	<properties>
		<targetJdk>1.7</targetJdk>

		<spring.security.oauth.version>3.19</spring.security.oauth.version>
		<spring.version>3.0.5.RELEASE</spring.version>
		<tiles.version>2.1.3</tiles.version>
