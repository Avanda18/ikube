		<logback.version>0.9.15</logback.version>
		<junit.version>4.8.2</junit.version>
		<encoding>UTF8</encoding>
		<project.build.sourceEncoding>${encoding}</project.build.sourceEncoding>

		<write.classes>-Dwrite.classes=true</write.classes>
		<clean.classes>-Dclean.classes=true</clean.classes>
		<included.packages>-Dincluded.packages=ikube:edu.uci</included.packages>
		<excluded.packages>-Dexcluded.packages=model:Mock:Test:Integration:sshxcute</excluded.packages>
		<included.adapters>-Dincluded.adapters=coverage:complexity:dependency</included.adapters>
		<serenity.parameters>${included.adapters} ${included.packages} ${excluded.packages} ${write.classes} ${clean.classes}</serenity.parameters>
		<memory.parameters>-XX:PermSize=256m -XX:MaxPermSize=1024m -Xms1024m -Xmx4096m</memory.parameters>

		<war.base.root>code/war/target/ikube</war.base.root>
		<postgresql.driver.version>9.1-901-1.jdbc4</postgresql.driver.version>
